{
  "name": "fitness",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "ci": "bun run typecheck && bun run lint && bun run test && bun run test:e2e",
    "db:dev": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx app/db/migrate.ts",
    "db:seed": "tsx app/db/index.ts",
    "dev": "react-router dev",
    "fmt": "biome format --write",
    "lint": "biome lint --write ./app",
    "prepare": "husky",
    "start": "react-router-serve ./build/server/index.js",
    "tc": "bun typecheck",
    "tc:watch": "nodemon --exec 'bun tc' --watch src --ext ts,tsx,js,jsx",
    "test": "vitest run",
    "test:debug": "vitest --reporter=verbose --silent=false",
    "test:e2e": "playwright test",
    "typecheck": "react-router typegen && tsc"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "bun fmt",
      "bun lint"
    ]
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.62.0",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/themes": "^3.2.1",
    "@react-router/node": "^7.11.0",
    "@react-router/serve": "^7.11.0",
    "@types/lodash": "^4.17.21",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.7",
    "fuse.js": "^7.1.0",
    "isbot": "^5.1.32",
    "lodash": "^4.17.21",
    "lucide-react": "^0.544.0",
    "neverthrow": "^8.2.0",
    "pg": "^8.16.3",
    "pino": "^10.1.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router": "^7.11.0",
    "recharts": "^3.6.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "@playwright/browser-chromium": "^1.58.1",
    "@playwright/browser-firefox": "^1.58.1",
    "@playwright/browser-webkit": "^1.58.1",
    "@playwright/test": "^1.58.1",
    "@react-router/dev": "^7.11.0",
    "@types/node": "^24.10.4",
    "@types/pg": "^8.16.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "drizzle-kit": "^0.31.8",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "nodemon": "^3.1.11",
    "pino-pretty": "^13.1.3",
    "sharp": "^0.34.5",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vite-tsconfig-paths": "^6.0.4",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">22.0.0"
  }
}
